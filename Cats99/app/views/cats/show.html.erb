<a href='<%= edit_cat_url(@cat) %>'> Edit Cat </a>

<table>
  <thead>
    <tr>
        <th colspan="2"><%= @cat.name %></th>
    </tr>
  </thead>
  <tbody>
    <tr>
        <td>Attribute</td>
        <td>Value</td>
    </tr>
    <tr>
        <td>Age</td>
        <td><%= @cat.age %></td>
    </tr>
    <tr>
        <td>Birth Date</td>
        <td><%= @cat.birth_date %></td>
    </tr>
    <tr>
        <td>Color</td>
        <td><%= @cat.color %></td>
    </tr>
    <tr>
        <td>Sex</td>
        <td><%= @cat.sex %></td>
    </tr>
    <tr>
        <td>Description</td>
        <td><%= @cat.description %></td>
    </tr>
  </tbody>
</table>

<a href='<%= cats_url %>'> Back to Home </a>
